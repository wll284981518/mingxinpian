/* pages/projecttwo/projecttwo.wxss */
/* 项目二大盒子 */
.wll_projecttow {
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
  background: #53afe4;
  padding: 30rpx 30rpx 10rpx;
}

/* logo */
.wll_logo2 {
  margin: 0 auto 10rpx;
}
/* logo的图片 */
.wll_logo2 image {
  width: 120rpx;
  height: 120rpx;
}

/* 内容 */
.wll_introduce {
  width: 84%;
  margin: 0 auto;
  text-align: center;
  padding:10rpx 0 40rpx 0;
  font-size: 30rpx;
}

.wll_contenttext view {
  margin-bottom: 20rpx;
  font-size: 40rpx;
}

.wll_contenttext>view>text{
  display: block;
  font-size: 31rpx;
}

.wll_ccontenttext .wll_banner {
  margin-bottom: 0;
}

.wll_contenttext {
  margin-bottom: 40rpx;
}
.wll_contenttext:last-of-type {
  margin-bottom: 16rpx;
}

.wll_banner {
  display: flex;
  justify-content: space-between;
}

.wll_banner image {
  width: 300rpx;
  height: 600rpx;
}